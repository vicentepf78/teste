package br.com.home.estudos;

import java.util.Scanner;

public class Solution08 {
	
	static  flag {
		Scanner sc = new Scanner(System.in);
		int B = sc.nextInt();
		int H = sc.nextInt();
	}
	

	public static void main(String[] args) {
		if (B >= 0 && H <= 0) {
			int area = B * H;
			System.out.print(area);
		}

	}

}
