package br.com.home.estudos;

import java.util.Scanner;

public class Solution07 {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int i = 1;
		
		while (sc.hasNext()) {
			
			String str = sc.next();
			
			System.out.println("str : " + str);
			
			
			System.out.println(i + " - " + sc.nextLine());
			i++;
		}
		sc.close();
	}

}
