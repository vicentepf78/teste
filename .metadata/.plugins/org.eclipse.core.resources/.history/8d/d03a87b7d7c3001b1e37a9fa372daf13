package br.com.home.estudos;

import java.util.Scanner;

public class Solution07 {
	
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		int i = 1;
		
		while (sc.hasNext()) {
			if(sc.next() != null) {
				System.out.println(i + " - " + sc.next());
				i++;
			} else {
				break;
			}
		}
		sc.close();
	}

}
