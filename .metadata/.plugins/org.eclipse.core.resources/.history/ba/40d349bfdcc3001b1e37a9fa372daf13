import java.util.Scanner;

public class Solution08 {
	
	static {
		Scanner sc = new Scanner(System.in);
		int B = sc.nextInt();
		int H = sc.nextInt();
	}
	

	public static void main(String[] args) {
		if (flag) {
			int area = B * H;
			System.out.print(area);
		}

	}

}
