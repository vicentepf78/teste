package br.com.home.estudos;

import java.util.Scanner;

public class Solution08 {
	
	public static int B;
	public static int H;
	
	static  {
		Scanner sc = new Scanner(System.in);
		B = sc.nextInt();
		H = sc.nextInt();
	}
	

	public static void main(String[] args) {
		if (B >= 0 && H >= 0) {
			int area = B * H;
			System.out.print(area);
		}

	}

}
